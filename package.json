{
  "name": "stream-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint ./src",
    "format": "biome format ./src",
    "check": "biome check ./src",
    "lint:fix": "biome lint --apply ./src",
    "format:fix": "biome format ./src --write",
    "check:fix": "biome check --apply ./src",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "lefthook": "^1.10.1",
    "typescript": "^5"
  },
  "volta": {
    "node": "22.13.0"
  }
}
